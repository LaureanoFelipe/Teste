{
  "family": "alfatech",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::746386302275:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::746386302275:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "alfa",
      "image": "746386302275.dkr.ecr.us-east-2.amazonaws.com/alfa:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "cpu": 0,
      "memory": null,
      "memoryReservation": null
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512",
  "runtimePlatform": {
    "operatingSystemFamily": "LINUX",
    "cpuArchitecture": null
  }
}